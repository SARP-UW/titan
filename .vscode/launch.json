{
    "version":"0.2.0",
    "configurations": [
        {
            "name": "st-link dev debug",
            "cwd": "${workspaceRoot}//dev",
            "executable": "${workspaceFolder}//dev//build//TITAN_DEV.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "stlinkPath": "{env:STLINK_PATH}/st-util.exe",
            "armToolchainPath": "{env:ARM_GNU_TOOLCHAIN}",
            "device": "STM32H745_CM7",
            "svdFile": "${workspaceFolder}//dev//resource//STM32H745_CM7.svd",
        },  
        {
          "name": "st-link test debug",
