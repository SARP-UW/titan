          "servertype": "stutil",
          "stlinkPath": "${env:STLINK_PATH}/st-util.exe",
          "armToolchainPath": "${env:ARM_GNU_TOOLCHAIN}",
          "device": "STM32H745ZIT6",
          "svdFile": "${workspaceFolder}/dev/resource/port.svd",
          "breakAfterReset": true,
          "targetProcessor": 1,
          "liveWatch": { "enabled": true, "samplesPerSecond": 4 },
        },
        {
          "name": "openocd dev debug CM7",
          "cwd": "${workspaceRoot}",
          "executable": "${workspaceFolder}/build/dev/TITAN_DEV.elf",
          "armToolchainPath": "${env:ARM_GNU_TOOLCHAIN}",
          "request": "attach",
          "type": "cortex-debug",
          "servertype": "openocd",
          "device": "STM32H745ZIT6",
          "configFiles": ["${workspaceFolder}/dev/resource/flash_config.cfg"],
          "svdFile": "${workspaceFolder}/dev/resource/port.svd",
          "breakAfterReset": true,
          "targetProcessor": 1,
